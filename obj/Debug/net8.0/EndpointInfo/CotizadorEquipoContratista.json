{
  "openapi": "3.0.1",
  "info": {
    "title": "API Cotización Equipos Contratistas",
    "version": "v1"
  },
  "paths": {
    "/api/Cotizacion/RegistrarMacro": {
      "post": {
        "tags": [
          "CotizadorEquipoContratista"
        ],
        "summary": "Registrar cotización de equipos contratistas",
        "description": "Envía una cotización a la API real y devuelve el resultado del registro.",
        "operationId": "RegistrarMacro",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CotizacionEquipoContratistaModel"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Respuesta exitosa del registro.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Void"
                }
              }
            }
          }
        }
      }
    },
    "/api/Cotizacion/GetToken": {
      "get": {
        "tags": [
          "CotizadorEquipoContratista"
        ],
        "summary": "Obtiene un token desde la API",
        "description": "Llama a la API CotizacionEquipoContratistaApi/GetToken para obtener un token válido.",
        "operationId": "GetToken",
        "parameters": [
          {
            "name": "X-API-KEY",
            "in": "header",
            "description": "API Key requerida para acceder al token",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
